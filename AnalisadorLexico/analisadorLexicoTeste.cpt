/* Programa para identificar o maior
 dentre dois números.
*/
vazio principal () {
 int a, b;
 tela <- "Informe o primeiro número: ";
 a <- teclado;
 tela <- "Informe o segundo número: ";
 b <- teclado;
 tela <- "Maior: " + maior (a, b);
}
int maior (int a, int b) {
 se (a > b)
 retornar (a);
 senao
 retornar (b);
}